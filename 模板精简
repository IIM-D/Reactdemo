const getHtag = (type) =>{
  if(type === 1){
    return <h1>上行</h1>
  }
  if(type === 2){
    return <h2>下行</h2>
  }
  if(type === 3){
    return <h3>定位</h3>
  }
}

function App() {
  return (
    <div className="App">
      {getHtag(1)}
      {getHtag(2)}
      {getHtag(3)}
    </div>
  )
}

export default App;
